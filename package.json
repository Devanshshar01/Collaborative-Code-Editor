{
  "name": "collaborative-editor",
  "version": "2.0.0",
  "description": "Production-grade collaborative code editor with VS Code features, Figma whiteboard, and Google Meet video",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "setup": "npm run setup:deps && npm run setup:docker",
    "setup:deps": "npm install",
    "setup:db": "node scripts/init-db.js",
    "setup:docker": "docker-compose -f docker/docker-compose.yml build",
    "validate:types": "tsc --noEmit --skipLibCheck",
    "validate:lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "validate:deps": "npm audit --audit-level=moderate",
    "validate:env": "node scripts/validate-env.js",
    "start": "node dist/server.js",
    "start:yjs": "node yjs-server-simple.js",
    "start:execution": "node execution-server.js",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "vite build",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:yjs\"",
    "dev:backend": "node --loader ts-node/esm --watch src/server.ts",
    "dev:frontend": "vite",
    "dev:yjs": "node yjs-server-simple.js",
    "preview": "vite preview",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:e2e": "playwright test",
    "test:load": "artillery run tests/load/artillery-config.yml",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --fix --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "docker:build": "docker-compose -f docker/docker-compose.yml build",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "docker:clean": "docker-compose -f docker/docker-compose.yml down -v --rmi local"
  },
  "keywords": [
    "collaborative",
    "code-editor",
    "real-time",
    "websocket",
    "crdt",
    "yjs",
    "whiteboard",
    "video-conferencing"
  ],
  "author": "Collaborative Editor Team",
  "license": "MIT",
