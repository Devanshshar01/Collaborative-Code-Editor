{
  "name": "collaborative-editor-server",
  "version": "1.0.0",
  "description": "Real-time collaborative code editor WebSocket server with Yjs CRDT support",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:yjs": "node dist/server-yjs.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:yjs": "ts-node-dev --respawn --transpile-only src/server-yjs.ts"
  },
  "keywords": [
    "websocket",
    "socket.io",
    "collaboration",
    "real-time"
  ],
  "author": "Gemini",
  "license": "ISC",
  "dependencies": {
    "socket.io": "^4.7.2",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "^4.7.2",
    "yjs": "^13.6.10",
    "y-protocols": "^1.0.6",
    "lib0": "^0.2.87",
    "mongodb": "^6.3.0",
    "ws": "^8.14.2",
    "dotenv": "^16.3.1",
    "@codemirror/view": "^6.22.0",
    "@codemirror/state": "^6.3.3",
    "@codemirror/lang-javascript": "^6.2.1",
    "@codemirror/lang-python": "^6.1.3",
    "@codemirror/lang-java": "^6.0.1",
    "@codemirror/commands": "^6.3.2",
    "@codemirror/language": "^6.9.3",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/autocomplete": "^6.11.1",
    "@codemirror/lint": "^6.4.2",
    "@codemirror/search": "^6.5.5",
    "@codemirror/collab": "^6.1.1",
    "y-codemirror.next": "^0.3.5",
    "simple-peer": "^9.11.1",
    "@tldraw/tldraw": "^2.0.0-beta.2",
    "y-indexeddb": "^9.0.12",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "lucide-react": "^0.292.0",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@types/simple-peer": "^9.11.6",
    "@types/ws": "^8.5.10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  }
}
